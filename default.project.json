{
  "name": "SAC AI",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "$ignoreUnknownInstances": true,
      "$className": "ReplicatedStorage",
      "Shared": {
        "$className": "Folder",
        "$path": "src/shared"
      },
      "Packages": {
        "$className": "Folder",
        "$path": "Packages"
      }
    },

    "ServerScriptService": {
      "$ignoreUnknownInstances": true,
      "$className": "ServerScriptService",
      "$path": "src/server/scripts"
    },
    
    "ServerStorage": {
      "$ignoreUnknownInstances": true,
      "$className": "ServerStorage",
      "$path": "src/server/storage"
    },
    
    "StarterPlayer": {
      "$className": "StarterPlayer",
      
      "StarterPlayerScripts": {
        "$ignoreUnknownInstances": true,
        "$className": "StarterPlayerScripts",
        "$path": "src/client/StarterPlayerScripts"
      },
      "StarterCharacterScripts": {
        "$ignoreUnknownInstances": true,
        "$className": "StarterCharacterScripts",
        "$path": "src/client/StarterCharacterScripts"
      }
      
    },
    
    
    
    "Workspace": {
      "$className": "Workspace",
      "$ignoreUnknownInstances": true,
      "$path": "src/client/workspace",
      "$properties": {
        "FilteringEnabled": true
      },
      
      "NPC_Waypoints": {
        "$className": "Folder",
        "$ignoreUnknownInstances": true,
        
        "EvadeWaypoints": {
          "$className": "Folder",
          "$ignoreUnknownInstances": true
        }
      },
      
      "NPC": {
        "$className": "Folder"
      },
      
      "GAME_WORKSPACE": {
        "$className": "Folder",
        "$ignoreUnknownInstances": true,
        "InvisWalls": {
          "$className": "Folder",
          "$ignoreUnknownInstances": true
        },
        "NPCAvoid": {
          "$className": "Folder",
          "$ignoreUnknownInstances": true
        }
      }
        
      
    }, 
    
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}